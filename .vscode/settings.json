{
  "editor.formatOnSave": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[java]": {
    "editor.defaultFormatter": "redhat.java",
    "editor.codeActionsOnSave": { "source.fixAll": true }
  },
  "cSpell.words": ["adacielochallenge", "cnpj", "Configurer", "prospec"],
  "cSpell.language": "en,pt,pt_BR",
  "java.autobuild.enabled": true,
  "java.compile.nullAnalysis.mode": "automatic",
  "java.completion.chain.enabled": true,
  "java.completion.enabled": true,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.edit.smartSemicolonDetection.enabled": true,
  "java.editor.reloadChangedSources": "auto",
  "java.maven.updateSnapshots": true,
  "maven.pomfile.autoUpdateEffectivePOM": true,
  "maven.view": "hierarchical"
}
